{
  "name": "@ntv/shared",
  "version": "0.7.0",
  "description": "Shared business logic, API clients, and domain models for nself-tv",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
