    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
